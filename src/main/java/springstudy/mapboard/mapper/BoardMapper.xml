<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="springstudy.mapboard.mapper.BoardMapper">

    <select id="selectBoardList" resultType="springstudy.mapboard.model.Board">
        SELECT
            id AS id,
            ttl AS title
        FROM brd
    </select>

</mapper>